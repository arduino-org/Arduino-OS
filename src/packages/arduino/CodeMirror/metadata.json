{
  "className":  "ApplicationCodeMirror",
  "name":       "CodeMirror",
  "mime":       [
    "^text",
    "inode\\/x\\-empty",
    "application\\/x\\-empty",
    "application\\/x\\-lua",
    "application\\/x\\-python",
    "application\\/x\\-shellscript",
    "text\\/x\\-c",
    "text\\/x\\-cplusplus",
    "application\\/javascript"
  ],

  "icon":       "arduino-ideino.png",
  "category":   "development",
  "preload":    [
    {"src": "vendor/CodeMirror/lib/codemirror.js", "type": "javascript"},
    {"src": "vendor/CodeMirror/mode/css/css.js", "type": "javascript"},
    {"src": "vendor/CodeMirror/mode/javascript/javascript.js", "type": "javascript"},
    {"src": "vendor/CodeMirror/mode/lua/lua.js", "type": "javascript"},
    {"src": "vendor/CodeMirror/mode/xml/xml.js", "type": "javascript"},
    {"src": "vendor/CodeMirror/mode/clike/clike.js", "type": "javascript"},
    {"src": "vendor/CodeMirror/mode/python/python.js", "type": "javascript"},
    {"src": "vendor/CodeMirror/mode/shell/shell.js", "type": "javascript"},
    {"src": "vendor/CodeMirror/lib/codemirror.css", "type": "stylesheet"},
    {"src": "vendor/CodeMirror/theme/material.css", "type": "stylesheet"},
    {"src": "main.js", "type": "javascript"},
    {"src": "main.css", "type": "stylesheet"}
  ]
}



